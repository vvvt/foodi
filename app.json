{
  "expo": {
    "name": "Foodi",
    "slug": "foodi",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "de.tu_dresden.app_dev.group_10.foodi",
      "versionCode": 6,
      "config": {
        "googleMaps": {
          "apiKey": "api-key"
        }
      }
    },
    "description": "A simple app to show all the meals of canteens around you in Dresden. A complete list of allergenes in your preferences that can be turned off and on however you want. A nice design, network traffic and battery friendly behaviour."
  }
}
